{
  "name": "nurpc",
  "version": "0.2.0",
  "description": "An implementation of gRPC protocol in Javascript",
  "main": "lib/nurpc.js",
  "devDependencies": {
    "istanbul": "~0.3",
    "jsdoc": "~3.3",
    "mocha": "~2.2"
  },
  "bin": {
    "nurpc_js_interop_client": "interop/interop_client.js",
    "math_client.js": "example/math_client.js",
    "math_server.js": "example/math_server.js"
  },
  "dependencies": {
    "argparse": "~1.0",
    "async": "~1.2",
    "bunyan": "~1.5",
    "chai": "~3.0",
    "concat-stream": "~1.4",
    "dirty-chai": "~1.2",
    "fs-extra": "~0.18",
    "google-auth-library": "~0.9",
    "http2": "tbetbetbe/node-http2#rpc-impl-all-changes",
    "jshint": "~2.5",
    "lodash": "~3.9",
    "protobufjs": "~4.1"
  },
  "scripts": {
    "doc": "jsdoc -c jsdoc.json",
    "interop_test": "istanbul test _mocha -- --reporter spec -g 'External Interop'",
    "lint": "jshint lib test",
    "prepare": "npm test && npm run lint && npm version patch",
    "test": "istanbul test _mocha -- --reporter spec -i -g 'External Interop' --slow 1000 --timeout 5000"
  },
  "author": "Timothy Emiola",
  "license": "MIT"
}
